{
	"name": "mediawiki/media-moderation",
	"description": "Service for filtering possible child exploitation content",
	"require": {},
	"require-dev": {
		"jakub-onderka/php-parallel-lint": "1.0.0",
		"jakub-onderka/php-console-highlighter": "0.4.0",
		"mediawiki/minus-x": "1.0.0",
		"mediawiki/mediawiki-codesniffer": "30.0.0",
		"mediawiki/mediawiki-phan-config": "0.9.2"
	},
	"scripts": {
		"test": [
			"parallel-lint . --exclude vendor --exclude node_modules",
			"minus-x check .",
			"phpcs -p -s"
		],
		"fix": [
			"minus-x fix .",
			"phpcbf"
		]
	},
	"extra": {
		"phan-taint-check-plugin": "2.0.2"
	}
}
